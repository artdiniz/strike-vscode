{
    "name": "strike-vscode",
    "displayName": "Strike for VSCode",
    "description": "Strikethrough text in a breeze",
    "version": "1.0.1",
    "publisher": "artdiniz",
    "author": {
        "name": "Artur Diniz Adam",
        "email": "artur.daz+vscodemarketplace@gmail.com"
    },
    "license": "MIT",
    "homepage": "https://github.com/artdiniz/strike-vscode/blob/master/README.md",
    "repository": {
       "type": "git",
       "url": "https://github.com/artdiniz/strike-vscode" 
    },
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Formatters",
        "Keymaps",
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.strike.strikethrough",
                "title": "Strikethrough selected text"
            }
        ],
        "keybindings":{
            "command": "extension.strike.strikethrough",
            "key": "ctrl+k s",
            "mac": "cmd+k s",
            "when": "editorHasSelection"
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
